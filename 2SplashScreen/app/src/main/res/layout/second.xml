<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/musicwall">

    <TextView
        android:id="@+id/ankit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:paddingTop="20dp"
        android:text="@string/ankit"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="35dp"

        app:layout_constraintBottom_toTopOf="@+id/mohit"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/mohit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="20dp"
        android:paddingTop="20dp"
        android:text="@string/mohit"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/rohit" />

    <TextView
        android:id="@+id/rohit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="20dp"
        android:text="@string/rohit"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/mohit" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="20dp"
        android:paddingTop="30dp"
        app:srcCompat="@drawable/environment"
        tools:layout_editor_absoluteX="-12dp"
        tools:layout_editor_absoluteY="-11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ankit"/>


</androidx.constraintlayout.widget.ConstraintLayout>